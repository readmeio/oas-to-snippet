{
  "name": "@readme/oas-to-snippet",
  "description": "Transform an OpenAPI operation into a code snippet",
  "version": "20.1.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Jon Ursenbach <jon@readme.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/readmeio/oas-to-snippet.git"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "prebuild": "rm -rf dist/",
    "prepack": "npm run build",
    "prepare": "husky install",
    "pretest": "npm run lint",
    "prettier": "prettier --list-different --write \"./**/**.{js,ts}\"",
    "release": "npx conventional-changelog-cli -i CHANGELOG.md -s && git add CHANGELOG.md",
    "test": "nyc mocha \"test/**/*.test.ts\"",
    "test:browser": "karma start --single-run",
    "test:browser:chrome": "karma start --browsers=Chrome --single-run=false",
    "test:browser:debug": "karma start --single-run=false"
  },
  "dependencies": {
    "@readme/httpsnippet": "^4.1.1",
    "@readme/oas-to-har": "^20.0.2",
    "httpsnippet-client-api": "^5.0.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@jsdevtools/host-environment": "^2.1.2",
    "@jsdevtools/karma-config": "^3.1.7",
    "@readme/eslint-config": "^10.3.2",
    "@readme/oas-examples": "^5.7.1",
    "@readme/oas-extensions": "^17.0.1",
    "@types/chai": "^4.3.1",
    "@types/har-format": "^1.2.8",
    "@types/mocha": "^10.0.0",
    "chai": "^4.3.6",
    "eslint": "^8.31.0",
    "har-examples": "^3.1.1",
    "husky": "^8.0.3",
    "mocha": "^10.2.0",
    "mocha-chai-jest-snapshot": "^1.1.4",
    "nyc": "^15.1.0",
    "oas": "^20.3.0",
    "prettier": "^2.8.1",
    "ts-loader": "^8.4.0",
    "ts-node": "^10.9.1",
    "type-fest": "^3.5.0",
    "typescript": "^4.9.4",
    "webpack": "^4.46.0"
  },
  "prettier": "@readme/eslint-config/prettier",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-line-length": [
        0,
        "always",
        "Infinity"
      ],
      "footer-max-line-length": [
        0,
        "always",
        "Infinity"
      ]
    }
  }
}
