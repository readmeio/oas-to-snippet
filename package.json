{
  "name": "@readme/oas-to-snippet",
  "description": "Transform an OpenAPI operation into a code snippet",
  "version": "21.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Jon Ursenbach <jon@readme.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/readmeio/oas-to-snippet.git"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "prebuild": "rm -rf dist/",
    "prepack": "npm run build",
    "prepare": "husky install",
    "pretest": "npm run lint",
    "prettier": "prettier --list-different --write \"./**/**.{js,ts}\"",
    "release": "npx conventional-changelog-cli -i CHANGELOG.md -s && git add CHANGELOG.md",
    "test": "jest --coverage"
  },
  "dependencies": {
    "@readme/httpsnippet": "^5.1.0",
    "@readme/oas-to-har": "^20.1.1",
    "httpsnippet-client-api": "^5.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@readme/eslint-config": "^10.5.1",
    "@readme/oas-examples": "^5.7.1",
    "@readme/oas-extensions": "^17.0.1",
    "@types/har-format": "^1.2.8",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.4",
    "eslint": "^8.35.0",
    "har-examples": "^3.1.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "oas": "^20.5.3",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "type-fest": "^3.6.1",
    "typescript": "^4.9.5"
  },
  "prettier": "@readme/eslint-config/prettier",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-line-length": [
        0,
        "always",
        "Infinity"
      ],
      "footer-max-line-length": [
        0,
        "always",
        "Infinity"
      ]
    }
  }
}
